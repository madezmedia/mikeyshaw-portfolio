{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { 
                "distDir": "dist" 
            }
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "/index.html" }
    ],
    "env": {
        "GITHUB_USERNAME": "@github_username"
    },
    "github": {
        "enabled": true,
        "silent": true
    }
}
